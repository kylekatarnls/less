{
    "name": "nodejs-php-fallback/less",
    "description": "PHP wrapper to execute less node package or fallback to a PHP alternative.",
    "type": "library",
    "authors": [
        {
            "name": "Kyle",
            "email": "kylekatarnls@gmail.com"
        }
    ],
    "require": {
        "nodejs-php-fallback/nodejs-php-fallback": "^1.2.5",
        "oyejorge/less.php": "^1.7, !=1.7.0.13"
    },
    "extra": {
        "npm": [
            "less",
            "clean-css"
        ]
    },
    "require-dev": {
        "composer/composer": "^1.2",
        "phpunit/phpunit": ">=4.8 <6.0",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "NodejsPhpFallback\\": "src/"
        }
    }
}
